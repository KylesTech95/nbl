<nav id="nav">
    <ul id="nav-ul">
        <% navlinks.map((link,index) => { %>
          <li class="nav-list-item">
                <a href="#" class="nav-list-link"> 
                    <% if(link==='Options'){ %>
                        <p> <%= link %></p>
                        <select name="event" id="select-event">
                            <option value="0">None</option>
                            <option value="1">Create an Event</option>
                        </select>

                        <div id="select-btn" class="margin-top-25 hidden">Go</div>
                    <% } else { %>
                        <p> <%= link %></p>
                        <% } %>
                </a>
          </li>
        <% }) %>
    </ul>
</nav>
